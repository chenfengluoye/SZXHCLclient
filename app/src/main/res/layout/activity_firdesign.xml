<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_weight="1">
        <com.qqjyb.szxhcl.DrawView
            android:visibility="gone"
            android:id="@+id/win"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_weight="1"/>
        <ImageView
            android:id="@+id/winshow"
            android:layout_width="match_parent"
            android:layout_height="0dp"
            android:layout_weight="1"
            android:src="@drawable/jxc"/>
        <View
            android:background="#79CEF1"
            android:layout_width="match_parent"
            android:layout_height="20dp"/>
    </LinearLayout>
    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">
        <LinearLayout
            android:gravity="center"
            android:background="@color/colorPrimary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <Spinner
                android:minHeight="40dp"
                android:id="@+id/firtype"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:entries="@array/firtype"
                android:gravity="center"
                android:textColor="#ffffff" />

            <Spinner
                android:id="@+id/wintype"
                android:minHeight="40dp"
                android:gravity="center"
                android:entries="@array/wintype"
                android:layout_weight="1"
                android:textColor="#ffffff"
                android:layout_width="0dp"
                android:layout_height="wrap_content" />
        </LinearLayout>
        <LinearLayout
            android:gravity="center"
            android:background="@color/colorPrimary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

            <LinearLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <TextView
                    android:gravity="center"
                    android:layout_weight="1"
                    android:text="阶数"
                    android:textColor="#ffffff"
                    android:background="#00000000"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/n"
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:text="30"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <TextView
                    android:gravity="center"
                    android:layout_weight="1"
                    android:text="频率(fln)"
                    android:textColor="#ffffff"
                    android:background="#00000000"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/fln"
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:text="30"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:background="@color/colorPrimary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <TextView
                    android:gravity="center"
                    android:layout_weight="1"
                    android:text="频率(fhn)"
                    android:textColor="#ffffff"
                    android:background="#00000000"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />

                <EditText
                    android:id="@+id/fhn"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="30"
                    android:textColor="#ffffff" />
            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <TextView
                    android:gravity="center"
                    android:layout_weight="1"
                    android:text="fs"
                    android:textColor="#ffffff"
                    android:background="#00000000"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/fs"
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:text="30"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:background="@color/colorPrimary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <TextView
                    android:gravity="center"
                    android:layout_weight="1"
                    android:text="通带波纹(Qp)"
                    android:textColor="#ffffff"
                    android:background="#00000000"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />

                <EditText
                    android:id="@+id/Qp"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1"
                    android:gravity="center"
                    android:text="30"
                    android:textColor="#ffffff" />
            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <TextView
                    android:gravity="center"
                    android:layout_weight="1"
                    android:text="阻带波纹(Qs)"
                    android:textColor="#ffffff"
                    android:background="#00000000"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/Qs"
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:text="30"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:background="@color/colorPrimary"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <LinearLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <TextView
                    android:gravity="center"
                    android:layout_weight="1"
                    android:text="β"
                    android:textColor="#ffffff"
                    android:background="#00000000"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:id="@+id/B"
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:text="30"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_gravity="center"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/shuoming"
                    android:gravity="center"
                    android:layout_weight="1"
                    android:text="参数说明"
                    android:textColor="#ffffff"
                    android:background="#00000000"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content" />
            </LinearLayout>
            <LinearLayout
                android:visibility="gone"
                android:layout_weight="1"
                android:layout_width="0dp"
                android:layout_height="wrap_content">
                <TextView
                    android:gravity="center"
                    android:layout_weight="1"
                    android:text="通带增益"
                    android:textColor="#ffffff"
                    android:background="#00000000"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
                <EditText
                    android:gravity="center"
                    android:textColor="#ffffff"
                    android:text="30"
                    android:layout_weight="1"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content" />
            </LinearLayout>
        </LinearLayout>
        <LinearLayout
            android:layout_weight="1"
            android:background="#5E88F8"
            android:layout_width="match_parent"
            android:minHeight="45dp"
            android:gravity="center"
            android:layout_height="wrap_content">
            <TextView
                android:id="@+id/cancel"
                android:paddingLeft="40dp"
                android:textSize="20sp"
                android:gravity="left"
                android:layout_weight="1"
                android:text="取消"
                android:textColor="#ffffff"
                android:background="#00000000"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
            <TextView
                android:id="@+id/save"
                android:textSize="20sp"
                android:gravity="right"
                android:paddingRight="40dp"
                android:textColor="#ffffff"
                android:text="保存"
                android:layout_weight="1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content" />
        </LinearLayout>
    </LinearLayout>

</LinearLayout>
